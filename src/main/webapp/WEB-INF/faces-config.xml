<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

   <navigation-rule>
    <navigation-case>
        <from-view-id>/showusers.xhtml</from-view-id>         
        <from-outcome>
           edituser
        </from-outcome>
        <to-view-id>/edituser.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
     <navigation-case>
        <from-view-id>/showapp.xhtml</from-view-id>         
        <from-outcome>
          addAppPage
        </from-outcome>
        <to-view-id>/addapp.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
     <navigation-case>
        <from-view-id>/addapp.xhtml</from-view-id>         
        <from-outcome>
          goToAppTable
        </from-outcome>
        <to-view-id>/showapp.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-view-id>/showpriv.xhtml</from-view-id>         
        <from-action>
             addPrivPage
        </from-action>
        <to-view-id>/addprivilege.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-view-id>/showusers.xhtml</from-view-id>         
        <from-action>
            addUserPage
        </from-action>
        <to-view-id>/adduser.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
     <navigation-case>
        <from-view-id>/addprivilege.xhtml</from-view-id>         
        <from-action>
             goToPrivTable
        </from-action>
        <to-view-id>/showpriv.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-view-id>/showroles.xhtml</from-view-id>         
        <from-action>
            addRolePage
        </from-action>
        <to-view-id>/addrole.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case> 
        <from-action>
            showUsers
        </from-action>
        <to-view-id>/showusers.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case> 
        <from-action>
            showRoles
        </from-action>
        <to-view-id>/showroles.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case> 
        <from-view-id>/adduser.xhtml</from-view-id>
        <from-action>
           goToUT
        </from-action>
        <to-view-id>/showusers.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case> 
        <from-view-id>/addrole.xhtml</from-view-id>
        <from-action>
           goToRoleTable
        </from-action>
        <to-view-id>/showroles.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
</navigation-rule>
</faces-config>
